# robot starts at (0, 2) facing -y direction (toward goal at (0, -2))
TurtleBot3Burger {
  translation 0 2 0
  rotation 0 0 1 -1.571
  name "MyRobot"
  controller "<extern>"
  extensionSlot [
    RobotisLds01 {
      name "Lidar"
    }
  ]
}
# pedestrians walking horizontal paths (perpendicular to robot's navigation direction)
# robot navigates from y=2 to y=-2, pedestrians walk left-right at various y values
DEF PEDESTRIAN1 Robot {
  translation 2.2 -1.8 0.15
  name "pedestrian1"
  supervisor TRUE
  controller "pedestrian_simple"
  controllerArgs [
    "--start=2.2,-1.8"
    "--end=-2.2,-1.8"
    "--speed=0.38"
  ]
  children [
    DEF BODY1 Shape {
      appearance PBRAppearance {
        baseColor 0.9 0.2 0.2
        roughness 0.5
      }
      geometry Cylinder {
        height 0.3
        radius 0.08
      }
    }
  ]
  boundingObject USE BODY1
  physics Physics {
    density -1
    mass 1
  }
}
